{
  "name": "@terminusdb/terminusdb-console",
  "version": "4.3.2",
  "homepage": "./",
  "description": "console for terminusdb",
  "main": "src/index",
  "module": "es6/index",
  "jsnext:main": "es6/index",
  "files": [
    "*.md",
    "src",
    "console",
    "es6"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.9.0"
  },
  "keywords": [
    "react",
    "reactjs",
    "chart",
    "react-component",
    "terminusdb"
  ],
  "scripts": {
    "build": "webpack --mode production --config console/webpack.config.js",
    "serve": "webpack-dev-server --mode development  --open --progress --port 3005 --host localhost --content-base console --inline --config console/webpack.config.js --history-api-fallback",
    "start": "webpack-dev-server --mode development  --open --progress --port 3005 --host localhost --content-base console --inline --config console/webpack.dev.config.js --history-api-fallback",
    "test": "exit 0",
    "lint": "eslint src",
    "autofix": "eslint src --fix",
    "cypress:open": "cypress open",
    "cypress:one": "cypress run --browser chrome --spec $1",
    "cypress:single": "cypress run --browser chrome --spec 'cypress/integration/tests/*.spec.js'",
    "cypress:run": "cypress run"
  },
  "pre-commit": [],
  "pre-push": [
    "test",
    "lint"
  ],
  "repository": {},
  "author": "terminusdb group",
  "peerDependencies": {
    "react": "^16.10.3",
    "react-dom": "^16.10.3"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.14.0",
    "@fonticonpicker/react-fonticonpicker": "^1.2.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "@terminusdb/terminusdb-client": "^4.3.1",
    "@terminusdb/terminusdb-react-components": "^4.3.1",
    "axios": "^0.21.1",
    "codemirror": "^5.60.0",
    "date-fns": "^2.19.0",
    "dotenv-webpack": "^1.8.0",
    "moment": "^2.29.1",
    "pexels": "^1.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.3",
    "react-bootstrap": "^1.5.2",
    "react-codemirror2": "^7.2.1",
    "react-data-table-component": "^6.11.7",
    "react-dom": "^16.10.3",
    "react-hook-form": "^4.10.2",
    "react-icons": "^3.11.0",
    "react-outside-click-handler": "^1.3.0",
    "react-papaparse": "^3.8.0",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.1.1",
    "react-transition-group": "^4.4.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/plugin-transform-regenerator": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "@babel/runtime-corejs2": "^7.13.10",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.0",
    "babel-preset-airbnb": "^2.6.0",
    "copy-webpack-plugin": "^5.1.2",
    "css-loader": "^3.6.0",
    "cypress": "^4.12.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "less-loader": "^6.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "pre-commit": "^1.1.3",
    "prettier": "^2.1.2",
    "react-test-renderer": "^16.14.0",
    "style-loader": "^1.3.0",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.8.2"
  },
  "license": "APACHE"
}
