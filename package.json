{
  "name": "@terminusdb/terminusdb-console",
  "version": "0.0.1",
  "homepage": "./",
  "description": "console for terminusdb",
  "main": "src/index",
  "module": "es6/index",
  "jsnext:main": "es6/index",
  "sideEffects": [
    "./*/index.js"
  ],
  "files": [
    "*.md",
    "src",
    "es6"
  ],
  "keywords": [
    "react",
    "reactjs",
    "chart",
    "react-component",
    "terminusdb"
  ],
  "scripts": {
    "build-es6": "rimraf es6 && cross-env NODE_ENV=es6 babel ./src -d es6 --extensions '.js'",
    "build": "npm run build-es6 && webpack --mode production",
    "buildConsole": "webpack --mode production --config console/webpack.config.js",
    "start": "webpack-dev-server --mode development --open --progress --port 3005 --host localhost --content-base console --inline --config console/webpack.config.js",
    "test": "exit 0",
    "lint": "eslint src",
    "autofix": "eslint src --fix",
    "installClient:local": "npm install --no-save ../terminus-client"
  },
  "pre-commit": [],
  "pre-push": [
    "test",
    "lint"
  ],
  "repository": {},
  "author": "terminusdb group",
  "peerDependencies": {
    "react": "^16.10.3",
